<?xml version='1.0' encoding='utf-8'?>
<document>
    <rule name="Vriska" color="#005682">
        <alias value="AG"/>
        <quirk from="[bB]" to="8"/>
        <quirk from=":" to="::::"/>
        <dequirk from="::::" to=":"/>
    </rule>
    <rule name="Karkat" color="#696969">
        <alias value="CG"/>
        <quirk from="(.)" to="\1"/>
        <dequirk from="[A-Z]" to="[a-z]"/>
    </rule>
    <rule name="Terezi" color="#008282">
        <alias value="GC"/>
        <alias value="TEREZI"/>
        <quirk from="[aA]" to="4"/>
        <quirk from="[iI]" to="1"/>
        <quirk from="[eE]" to="3"/>
        <dequirk from="4" to="a"/>
        <dequirk from="1" to="i"/>
        <dequirk from="3" to="e"/>
    </rule>
</document>
